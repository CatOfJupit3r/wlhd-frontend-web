{
    "trailingComma": "all",
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "plugins": ["prettier-plugin-classnames", "prettier-plugin-tailwindcss", "@trivago/prettier-plugin-sort-imports"],
    "endOfLine": "crlf",
    "importOrderSeparation": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@(components|configuration|locales|constants|hooks|models|mutations|pages|queries|router|services|context|utils|jotai-atoms)(.*)$",
        "^[./]"
    ]
}
